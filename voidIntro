#include <iostream>     //std::cout
#include <string>       //std::string
#include <vector>       //std::vector
#include <algorithm>    //std::sort
#include <array>        //std::array


// refer http://www.cplusplus.com/doc/tutorial/functions/

void printmessage(void) // or this is same as "...void printmessage(void)
{
    std::cout << "I am printing this non-value returning void function";
}

int main ()
{
    printmessage();
    std::cout << std::endl;
    return 0;
}
